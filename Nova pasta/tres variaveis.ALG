Algoritmo "semnome"
Var
// Seção de Declarações das variáveis
  a,b,c: inteiro
  m : real

Inicio
// Seção de Comandos, procedimento, funções, operadores, etc...
    escreval("informe um numeros inteiros: ")

    leia(a,b,c)

    m <- (a+b-c)

    escreval("O resultado de ",a," mais ",b," menos ",c," e igual a ",m,".")

Fimalgoritmo