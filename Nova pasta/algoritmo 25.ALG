Algoritmo "25"
Var
// Seção de Declarações das variáveis
  a,b: inteiro
  m : real

Inicio
// Seção de Comandos, procedimento, funções, operadores, etc...
    escreval("informe um numeros inteiros: ")

    leia(a)

    m <- (a)/4

    escreval("25% de ",a," e igual a ",m,".")


Fimalgoritmo