Algoritmo "subtracao"
Var
// Seção de Declarações das variáveis
  a,b: inteiro
  m : real

Inicio
// Seção de Comandos, procedimento, funções, operadores, etc...
    escreval("informe um numeros inteiros: ")

    leia(a,b)

    m <- (a-b)

    escreval("O resutado de ",a," e ",b," e igual a ",m,".")


Fimalgoritmo